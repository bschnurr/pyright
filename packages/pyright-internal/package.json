{
    "name": "pyright-internal",
    "displayName": "pyright",
    "description": "Type checker for the Python language",
    "version": "1.1.403",
    "license": "MIT",
    "private": true,
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "clean": "shx rm -rf ./dist ./out",
        "webpack:testserver": "webpack --config ./src/tests/lsp/webpack.testserver.config.js --mode=development",
        "webpack:testserver:watch": "npm run clean && webpack --config ./src/tests/lsp/webpack.testserver.config.js --mode development --watch --progress",
        "test": "npm run webpack:testserver && jest --forceExit",
        "test:norebuild": "jest --forceExit",
        "test:coverage": "jest --forceExit --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --coverageReporters=html --coverageReporters=json",
        "test:imports": "jest importResolver.test --forceExit --runInBand",
        "test:memory": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096 --expose-gc --inspect\" jest --testPathPattern=memory --runInBand --detectOpenHandles --forceExit",
        "test:memory:report": "npm run test:memory -- --verbose --reporters=default",
        "test:memory:parser": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096 --expose-gc --inspect\" jest src/tests/memory/parser.memory.test.ts --runInBand --detectOpenHandles --forceExit --verbose",
        "test:memory:cli": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144 --expose-gc --inspect\" jest src/tests/memory/cli-analysis.memory.test.ts --runInBand --detectOpenHandles --forceExit --verbose --timeout=300000",
        "pprof:help": "echo 'Install pprof CLI: go install github.com/google/pprof@latest' && echo 'View profiles: pprof -http=:8080 <profile.pb.gz>' && echo 'Generate SVG: pprof -svg <profile.pb.gz> > flame.svg' && find . -name '*.pb.gz' -type f | head -5 && echo 'Use: npm run pprof:view <profile-name>'",
        "pprof:view": "echo 'Opening profile in browser at http://localhost:8080' && pprof -http=:8080",
        "pprof:list": "echo 'ðŸ“Š Available Profile Files:' && find . -name '*.pb.gz' -type f -exec ls -lh {} \\; && echo '' && echo 'To analyze online: Upload to https://pprof.me/' && echo 'To install pprof: brew install go graphviz && go install github.com/google/pprof@latest'",
        "pprof:install": "echo 'Installing Go, Graphviz, and pprof...' && brew install go graphviz && go install github.com/google/pprof@latest && echo 'Done! Now you can use: pprof -http=:8080 <profile.pb.gz>'"
    },
    "dependencies": {
        "@yarnpkg/fslib": "2.10.4",
        "@yarnpkg/libzip": "2.3.0",
        "chalk": "^4.1.2",
        "chokidar": "^3.6.0",
        "command-line-args": "^5.2.1",
        "jsonc-parser": "^3.3.1",
        "smol-toml": "^1.3.1",
        "source-map-support": "^0.5.21",
        "tmp": "^0.2.3",
        "vscode-jsonrpc": "^9.0.0-next.5",
        "vscode-languageserver": "^10.0.0-next.10",
        "vscode-languageserver-protocol": "^3.17.6-next.10",
        "vscode-languageserver-textdocument": "^1.0.11",
        "vscode-languageserver-types": "^3.17.6-next.5",
        "vscode-uri": "^3.0.8"
    },
    "devDependencies": {
        "@types/command-line-args": "^5.2.3",
        "@types/fs-extra": "^11.0.4",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.14",
        "@types/node": "^22.10.5",
        "@types/tmp": "^0.2.6",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^7.0.3",
        "esbuild-loader": "^3.2.0",
        "fs-extra": "^11.2.0",
        "jest": "^29.7.0",
        "jest-junit": "^16.0.0",
        "pprof": "^3.2.1",
        "shx": "^0.3.4",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "typescript": "~5.5.4",
        "webpack": "^5.97.1",
        "webpack-cli": "^5.1.4",
        "word-wrap": "1.2.5"
    }
}
